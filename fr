Calendario_Trading =
VAR MinFecha =
    CALCULATE(
        MIN( 'tablones_des tabla_trading'[Fecha] ),
        ALL( 'tablones_des tabla_trading' )
    )
VAR MaxFecha =
    CALCULATE(
        MAX( 'tablones_des tabla_trading'[Fecha] ),
        ALL( 'tablones_des tabla_trading' )
    )
RETURN
SELECTCOLUMNS(
    CALENDAR( MinFecha, MaxFecha ),
    "Date",    [Date],
    "Mes-AÃ±o", FORMAT( [Date], "MMM-yy", "es-ES" )
)
