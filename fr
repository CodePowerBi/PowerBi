Fecha_UltimoCierreMesTrimestre =
CALCULATE(
    MAX ( 'tablones_des tabla_trading'[Fecha] ),
    FILTER(
        ALL ( 'tablones_des tabla_trading' ),
        YEAR ( 'tablones_des tabla_trading'[Fecha] ) = YEAR  ( 'Calendario_Trading'[Date] ) &&
        MONTH( 'tablones_des tabla_trading'[Fecha] ) = MONTH ( 'Calendario_Trading'[Date] )
    )
)
