MesAño_UltimoMesTrimestre =
CALCULATE (
    MAX ( 'tablones_des tabla_trading'[MesAño_UltimoMesTrimestre] ),
    FILTER (
        'tablones_des tabla_trading',
        'tablones_des tabla_trading'[Fecha] = 'Calendario_Trading'[Date]
    )
)


__________-


Indice_MesAnio = YEAR ( 'Calendario_Trading'[Date] ) * 100 + MONTH ( 'Calendario_Trading'[Date] )
