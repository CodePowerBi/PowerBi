Categoria_Grafico = 
VAR texto = UPPER(dicc_metrica[cat_3])
RETURN
SWITCH(
    TRUE(),
    CONTAINSSTRING(texto, "MANTENIDOS PARA NEGOCIAR"), "Held for trading",
    CONTAINSSTRING(texto, "DESIGNADOS A VR CON CAMBIO EN RESULTADOS"), "FV through P&L",
    CONTAINSSTRING(texto, "A VR CON CAMBIO EN OTRO RESULTADO GLOBAL"), "FV through OCI",
    CONTAINSSTRING(texto, "CONTABILIDAD DE COBERTURAS"), "Hedging derivatives",
    CONTAINSSTRING(texto, "SEGUROS Y REASEGUROS"), "Liabilities under insurance contracts",
    "Otros"
)
