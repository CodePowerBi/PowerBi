BanderasConTotal = 
VAR UrlSpain = 
    CALCULATE(
        MAX(Banderas[URL]),
        Banderas[Country_Limpio] = "Spain (*)"
    )

RETURN
UNION(
    Banderas,
    ROW(
        "Country", "Total",
        "URL", UrlSpain,
        "Codigo Pais", "TOTAL",
        "Country_Limpio", "Total",
        "OrdenNombre", 1
    )
)
