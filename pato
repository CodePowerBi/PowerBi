EsUltimoTrimestreCompleto = 
VAR FechaActual = MAX('tablones_des_vista_salida_daily_lcr_ddbb'[date])
VAR MesActual = MONTH(FechaActual)
VAR AnioActual = YEAR(FechaActual)
VAR TrimestreActual = INT((MesActual - 1) / 3) + 1

VAR AnioTrimestreAnterior = IF(TrimestreActual = 1, AnioActual - 1, AnioActual)
VAR TrimestreAnterior = IF(TrimestreActual = 1, 4, TrimestreActual - 1)

VAR FechaInicioTrimestreAnterior = 
    DATE(AnioTrimestreAnterior, (TrimestreAnterior - 1) * 3 + 1, 1)

VAR FechaFinTrimestreAnterior = 
    EOMONTH(DATE(AnioTrimestreAnterior, TrimestreAnterior * 3, 1), 0)

RETURN
IF(
    'tablones_des_vista_salida_daily_lcr_ddbb'[date] >= FechaInicioTrimestreAnterior &&
    'tablones_des_vista_salida_daily_lcr_ddbb'[date] <= FechaFinTrimestreAnterior,
    1,
    0
)
