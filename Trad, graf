Month_ALERT_Por_Pais :=
VAR FechaMax =
    CALCULATE(
        MAX('tablon_des_tabla_trading'[fecha]),
        ALLEXCEPT('tablon_des_tabla_trading', 'Country_Trading'[Country_Limpio])
    )
VAR MesActual = MONTH(FechaMax)
VAR AnioActual = YEAR(FechaMax)
VAR TrimestreActual = INT((MesActual - 1) / 3) + 1
VAR AnioTrimestreAnterior = IF(TrimestreActual = 1, AnioActual - 1, AnioActual)
VAR TrimestreAnterior = IF(TrimestreActual = 1, 4, TrimestreActual - 1)
VAR UltimoMes =
    SWITCH(
        TrimestreAnterior,
        1, 3,
        2, 6,
        3, 9,
        4, 12
    )
VAR FechaUltimoMes = DATE(AnioTrimestreAnterior, UltimoMes, 1)

VAR Resultado =
    CALCULATE(
        MAX('tablon_des_tabla_trading'[valor]),
        'tablon_des_tabla_trading'[fecha] = FechaUltimoMes,
        'tablon_des_tabla_trading'[tipo] = "Alert",
        ALLEXCEPT('tablon_des_tabla_trading', 'Country_Trading'[Country_Limpio])
    )

RETURN
    IF(ISBLANK(Resultado), 0, ABS(Resultado))
