Dim_Cols =
SUMMARIZE(
    'Tabla medidas',
    'Tabla medidas'[CATEGORIA 1],
    'Tabla medidas'[Categoria 2],
    "Cat1_ORD", MAX('Tabla medidas'[Categoria 1 - Visual]),
    "Cat2_ORD", MAX('Tabla medidas'[ID ORDEN]),
    "Cat2_ORD_GLOBAL", MAX('Tabla medidas'[Categoria 1 - Visual]) * 1000 + MAX('Tabla medidas'[ID ORDEN]),
    "Cat2_Key", MAX('Tabla medidas'[Categoria 2]) & REPT( UNICHAR(8203), MAX('Tabla medidas'[Categoria 1 - Visual]) )
)
