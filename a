FechaMax_Slicer_Menos3_Text :=
VAR Fecha = [FechaMax_Slicer]
VAR Fecha_3M =
    IF (
        ISBLANK ( Fecha ),
        BLANK (),
        EDATE ( Fecha, -3 )
    )
VAR TextoRaw =
    IF (
        ISBLANK ( Fecha_3M ),
        BLANK (),
        FORMAT ( Fecha_3M, "MMMM yyyy", "en-US" )   -- ej: "March 2024"
    )
RETURN
TextoRaw
