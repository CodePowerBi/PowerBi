mrf_lcr_historico =
VAR MaxFechaLCR =
    CALCULATE(
        MAX ( 'lcr_historico'[effective_date] ),
        ALL ( 'lcr_historico' )
    )

VAR MaxFechaDate =
    MAX ( 'TABLA Effective Date'[Date] )

VAR FechaFinal =
    EOMONTH(
        IF(
            MaxFechaLCR > MaxFechaDate,
            MaxFechaDate,
            MaxFechaLCR
        ),
        0
    )

RETURN
    EOMONTH ( MaxFechaDate, 0 ) = FechaFinal
