InicioMes_Menos3 :=
VAR F = [FechaMax_Slicer]
RETURN IF( NOT ISBLANK(F), EOMONTH(F, -4) + 1 )   -- 01/mar/25 si F es jun-25

FinMes_Menos3 :=
VAR F = [FechaMax_Slicer]
RETURN IF( NOT ISBLANK(F), EOMONTH(F, -3) )       -- 31/mar/25 si F es jun-25


____________


Valor_Metrica_Mes_Menos3 :=
CALCULATE(
    SUM( 'salida_buffer_hqla_vw'[valor_metrica] ),
    DATESBETWEEN( Dates[Date], [InicioMes_Menos3], [FinMes_Menos3] )
)
