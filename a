% Funding MEDIDA :=
VAR amount =
    SUM ( 'pag7 nueva'[Amount Received (bn)] )              -- numerador
VAR pasivo =
    MAX ( 'pag7 nueva'[CantidadTotalPasivo] )               -- constante por mes â†’ MAX para no sumarlo N veces
VAR totalMes =
    MAX ( 'pag7 nueva'[total Amount recibido por fecha] )   -- idem
VAR den = COALESCE(pasivo,0) + COALESCE(totalMes,0)
RETURN
IF ( den = 0 || ISBLANK(amount), BLANK(), DIVIDE( amount, den ) )
