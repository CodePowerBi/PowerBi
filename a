Level1_con_TOTAL :=
VAR Epig =
    SELECTEDVALUE( 'Dicc_epigrafes_total'[Limpieza new] )
RETURN
IF (
    Epig = "TOTAL",
    -- Fila TOTAL: sumar SOLO estos 4 ep√≠grafes
    CALCULATE (
        [Level1],
        ALL ( 'Dicc_epigrafes_total'[Limpieza new] ),       -- quitar filtro "TOTAL"
        'Dicc_epigrafes_total'[Limpieza new] IN {
            "Central Banks",
            "Credit Institutions",
            "Customers",
            "Debt Instruments"
        }
    ),
    -- Fila normal: mismo valor que la medida Level1
    [Level1]
)
