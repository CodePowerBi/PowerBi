Dim_Cols =
VAR T =
    SELECTCOLUMNS(
        'Tabla Param',
        "Cat1",      'Tabla Param'[CATEGORIA 1],
        "Cat1_ORD",  'Tabla Param'[Categoria 1 - Visual],
        "Cat2",      'Tabla Param'[Categoria 2],
        "Cat2_ORD",  'Tabla Param'[ID ORDEN]
    )
RETURN
ADDCOLUMNS(
    T,
    "Cat2_ORD_GLOBAL", [Cat1_ORD] * 1000 + [Cat2_ORD]
)
