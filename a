Comentario_Unido :=
VAR HighTxt =
    FIRSTNONBLANK ( comment_js_vw[comentario high], LEN ( comment_js_vw[comentario high] ) )
VAR EveTxt  =
    FIRSTNONBLANK ( comment_js_vw[comentario eve] , LEN ( comment_js_vw[comentario eve]  ) )
RETURN
TRIM (
    COALESCE ( HighTxt, "" )
    & IF ( NOT ISBLANK ( EveTxt ), UNICHAR(13) & UNICHAR(10) & EveTxt, "" )
)
