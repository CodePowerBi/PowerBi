informe_regulatorio_copy =
VAR MaxEffDate =
    MAXX( 'datos_manuales_ajustados', 'datos_manuales_ajustados'[effective_date] )
RETURN
ADDCOLUMNS(
    FILTER(
        'informe_regulatorio',
        'informe_regulatorio'[fecha] <= MaxEffDate
    ),
    "MaxEffectiveDate", MaxEffDate,
    "MaxEffectiveLabel", FORMAT( MaxEffDate, "yyyy-MM-dd" )
)
