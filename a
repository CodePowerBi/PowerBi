SUP_DEF_CATEGORIA =
VAR total =
    CALCULATE(
        MAX('tablones_des_mrf_composition_per_currency_lcr_divisas'[sup_def]),
        'tablones_des_mrf_composition_per_currency_lcr_divisas'[divisa] = "TOT"
    )
VAR suma_6divisas =
    CALCULATE(
        SUM('tablones_des_mrf_composition_per_currency_lcr_divisas'[sup_def]),
        'tablones_des_mrf_composition_per_currency_lcr_divisas'[divisa] IN {
            "BRL", "CLP", "EUR", "GBP", "MXN", "USD"
        }
    )
VAR other = total - suma_6divisas
RETURN
SWITCH(
    TRUE(),
    SELECTEDVALUE('tablones_des_mrf_composition_per_currency_lcr_divisas'[divisa]) IN {
        "BRL", "CLP", "EUR", "GBP", "MXN", "USD"
    },
    SUM('tablones_des_mrf_composition_per_currency_lcr_divisas'[sup_def]),
    SELECTEDVALUE('tablones_des_mrf_composition_per_currency_lcr_divisas'[divisa]) = "OTHER",
    IF(other > 0, other, 0),
    BLANK()
)
