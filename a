Suma_Mes_Anterior =
VAR MesSeleccionado = SELECTEDVALUE(km1[MesAñoUltMesTrimestre])
VAR ListaMeses =
    DISTINCT(SELECTCOLUMNS(km1, "Mes", km1[MesAñoUltMesTrimestre]))
VAR PosSeleccionado =
    RANKX(
        ListaMeses,
        [Mes],
        ,
        DESC,
        DENSE
    )
VAR MesAnterior =
    MAXX(
        FILTER(
            ListaMeses,
            RANKX(ListaMeses, [Mes], , DESC, DENSE) = PosSeleccionado + 1
        ),
        [Mes]
    )
RETURN
IF(
    NOT(ISBLANK(MesSeleccionado)) && NOT(ISBLANK(MesAnterior)),
    CALCULATE(
        SUM(km1[cantidad]),
        km1[MesAñoUltMesTrimestre] = MesAnterior
    ),
    BLANK()
)
