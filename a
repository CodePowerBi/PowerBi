Valor_CET1_OnlyTotal =
IF (
    HASONEVALUE('TABLA_NUEVA_CET'[Metrica_Nombre]),
    SUM('TABLA_NUEVA_CET'[valor]),  -- devuelve normal cuando es una fila concreta
    CALCULATE(
        SUM('TABLA_NUEVA_CET'[valor]),
        'TABLA_NUEVA_CET'[Metrica_Nombre] = "CET1"
    )  -- solo CET1 cuando estamos en el total
)
