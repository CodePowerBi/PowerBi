id_unit_GBUSA_distinct = 
VAR IdBuscado = dicc_id_bsi[id_bsi]
RETURN
CONCATENATEX(
    DISTINCT(
        FILTER(
            usa_liquidez_vw,
            usa_liquidez_vw[id_bsi] = IdBuscado &&
            CONTAINSSTRING(usa_liquidez_vw[id_unit], "GBUSA")
        )
    ),
    usa_liquidez_vw[id_unit],
    " - "
)
