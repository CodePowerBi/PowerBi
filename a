% Funding :=
VAR amount = 'Pag7_Top10_mensual'[amount_received]    -- o '...[Amount - Millones]' si es columna
VAR pasivo = 'Pag7_Top10_mensual'[CantidadTotalPasivo]
VAR mes    = 'Pag7_Top10_mensual'[Amount_Month_Sum]
VAR den    = COALESCE(pasivo,0) + COALESCE(mes,0)
RETURN IF( OR(ISBLANK(amount), den = 0), BLANK(), amount / den )
