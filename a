ValorIndicador_Num = 
VAR indicador = SELECTEDVALUE(pag5[Item])
RETURN
    SWITCH(
        indicador,
        "SUP/DEF", MAX('tablones_des mrf_composition_per_currency'[sup_def]),
        "RATIO", MAX('tablones_des mrf_composition_per_currency'[ratio]) / 100,
        "HQLA", MAX('tablones_des mrf_composition_per_currency_lcr_divisas'[hqla]),
        "CASH FLOWS", MAX('tablones_des mrf_composition_per_currency_lcr_divisas'[cash_flows]),
        BLANK()
    )
