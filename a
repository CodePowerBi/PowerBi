Current MTD FVPL =
VAR _Last =
    MAX ( 'DICC_FECHA'[Date] )          -- fecha m√°xima dentro del contexto del slicer

VAR _Start =
    DATE ( YEAR ( _Last ), MONTH ( _Last ), 1 )

VAR _total =
    CALCULATE (
        SUM ( vw_equity_2[sum_imrazonable] ),
        vw_equity_2[nbcartera] = "29 - FVPL",
        DATESBETWEEN ( 'DICC_FECHA'[Date], _Start, _Last )
    )
RETURN
    COALESCE ( _total, 0 )
