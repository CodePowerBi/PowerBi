CleanHighlights = 
VAR txt = 'comment_js_vw'[comment_highlights]
VAR salto = UNICHAR(10)
VAR limpio = 
    SUBSTITUTE(
        SUBSTITUTE(
            SUBSTITUTE(txt, salto & salto & salto, salto),
            salto & salto,
            salto
        ),
        salto & salto,
        salto
    )
RETURN limpio
