Porc_1_TotalColumna :=
VAR TotalActivos  = [total activos]
VAR TotalColumna  = [total columna]
RETURN
IF (
    NOT ISBLANK ( TotalActivos ) && TotalActivos <> 0,
    DIVIDE ( TotalColumna, TotalActivos ),
    BLANK()
)


__________

Porc_2_Diferencia :=
VAR TotalActivos  = [total activos]
VAR TotalColumna  = [total columna]
VAR Diferencia    = TotalActivos - TotalColumna
RETURN
IF (
    NOT ISBLANK ( TotalActivos ) && TotalActivos <> 0,
    DIVIDE ( Diferencia, TotalActivos ),
    BLANK()
)
