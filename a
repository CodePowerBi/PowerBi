Orden_Epigrafe =
VAR t = Dicc_Epigrafes_Total[epigrafe_front_total]
RETURN
SWITCH (
    TRUE(),
    CONTAINSSTRING ( t, "Central Banks" ), 1,
    CONTAINSSTRING ( t, "Credit institutions" ), 2,
    CONTAINSSTRING ( t, "Customers" ), 3,
    CONTAINSSTRING ( t, "Market debt securities" ), 4,
    CONTAINSSTRING ( t, "Subliabilities" ) || CONTAINSSTRING ( t, "Sub liabilities" ), 5,
    CONTAINSSTRING ( t, "Other financial" ), 6,
    999
)
