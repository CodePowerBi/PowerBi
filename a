Fusion_Level_MesPaisSoc :=
VAR BaseFiltrada =
    FILTER (
        fusion_vw,
        fusion_vw[id_epigrafe] IN { 1, 2, 3, 4, 5, 7, 9, 11, 13, 14 }
    )
RETURN
SUMMARIZE (
    BaseFiltrada,
    fusion_vw[report_date],
    fusion_vw[id_pais],
    fusion_vw[id_sociedad],
    fusion_vw[extr_date],
    "Level1_Total", SUM ( fusion_vw[lvl1] ),
    "Level2_Total", SUM ( fusion_vw[lvl2] ),
    "Level3_Total", SUM ( fusion_vw[lvl3] )
)
