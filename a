%Funding = 
VAR TotalAmountRecibido = 
    CALCULATE(
        SUM('mrf_plantilla_67'[amount_recibido]),
        REMOVEFILTERS('mrf_plantilla_67'[Counterparty Name])  // Solo quita el filtro de contraparte, mantiene fecha
    )

VAR CantidadTotalPasivo = 
    CALCULATE(
        SUM('mrf_pasivo_plantilla_67'[cantidad]),
        REMOVEFILTERS('mrf_plantilla_67'[Counterparty Name])  // Lo mismo
    )

VAR CantidadAjustada = CantidadTotalPasivo - TotalAmountRecibido

VAR AmountRecibido = 
    SUM('mrf_plantilla_67'[amount_recibido])  // Respetando la contraparte

RETURN
DIVIDE(AmountRecibido, CantidadAjustada)
