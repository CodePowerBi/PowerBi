Level2_con_TOTAL_ASSETS =
VAR _Ids = { 6, 7, 10, 13 }
RETURN
SWITCH (
    TRUE (),
    SELECTEDVALUE ( Dicc_Epigrafes_Total[id_epigrafe] ) = 999,
        CALCULATE (
            [• Level2.],
            TREATAS ( _Ids, fusion_vw[id_epigrafe] ),
            REMOVEFILTERS ( Dicc_Epigrafes_Total[epigrafe_front_total] )
        ),
    CALCULATE (
        [• Level2.],
        TREATAS ( _Ids, fusion_vw[id_epigrafe] )
    )
)
