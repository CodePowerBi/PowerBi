Level1_Eje_Meses :=
VAR Cat   = SELECTEDVALUE ( Eje_Meses[Categoria] )
VAR MaxYM = [MesMax_Slicer]
VAR TargetYM =
    SWITCH (
        Cat,
        "Actual",           MaxYM,
        "Anterior",         MaxYM - 1,
        "Dos_Meses_Antes",  MaxYM - 2,
        "Tres_Meses_Antes", MaxYM - 3,
        BLANK()
    )
RETURN
IF (
    ISBLANK ( TargetYM ),
    BLANK(),
    CALCULATE ( [Level1.], 'Dates'[MesAnioOrden] = TargetYM )
)
