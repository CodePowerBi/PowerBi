NotaControl =
SWITCH(
    TRUE(),
    mrf_liquidity[pct_diferencia] < 5, 1,          // compliant
    mrf_liquidity[pct_diferencia] < 15, 2,         // medio
    3                                              // non-compliant
)


____________


NotaMedia :=
AVERAGEX(
    mrf_liquidity,
    mrf_liquidity[NotaControl]
)

___________

KPI :=
SWITCH(
    TRUE(),
    [NotaMedia] < 1.66, 0,
    [NotaMedia] < 2.33, 0.5,
    1
)
