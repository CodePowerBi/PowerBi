SELECT 
    t.date,
    t.effective_date,
    t.calculation_date,
    t.filename,
    t.metrica_esp,
    SUM(t.cantidad) AS cantidad_total
FROM 
    tablones_des_mrf_km1 t
WHERE 
    t.effective_date = (
        SELECT MAX(effective_date)
        FROM tablones_des_mrf_km1
    )
GROUP BY 
    t.date,
    t.effective_date,
    t.calculation_date,
    t.filename,
    t.metrica_esp
ORDER BY
    t.date, t.metrica_esp;
