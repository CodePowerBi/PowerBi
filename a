Acumulado YTD (columna) :=
VAR d = 'main_sheet'[Fecha]
RETURN
CALCULATE(
    SUM ( 'main_sheet'[Valor] ),
    FILTER(
        ALLEXCEPT(
            'main_sheet',
            'main_sheet'[Metric],
            'main_sheet'[id_unit_curr],
            'main_sheet'[id_segment_aggr],
            'main_sheet'[id_source_currency]
        ),
        YEAR ( 'main_sheet'[Fecha] ) = YEAR ( d )     -- mismo a√±o
            && 'main_sheet'[Fecha] <= d               -- hasta la fecha actual
    )
)
