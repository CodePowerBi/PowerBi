RankingTop10_-3M :=
VAR ThisAmount = [Amount_-3M]
VAR RankCalc =
    RANKX (
        ALLSELECTED ( 'pag7 nueva'[CounterpartyName] ),
        [Amount_-3M],
        ThisAmount,
        DESC,
        SKIP
    )
RETURN
IF (
    ISBLANK ( ThisAmount ),
    BLANK (),
    IF ( RankCalc <= 10, RankCalc, BLANK() )
)
