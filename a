Ultimos6Meses = 
VAR MaxFecha = [Max]
VAR AnyoMax = YEAR(MaxFecha)
VAR MesMax = MONTH(MaxFecha)

VAR MesComparado = MONTH('Dates'[Date])
VAR AnyoComparado = YEAR('Dates'[Date])

VAR NMesesDiff = DATEDIFF(DATE(AnyoComparado, MesComparado, 1), DATE(AnyoMax, MesMax, 1), MONTH)

RETURN
IF (
    NMesesDiff >= 0 && NMesesDiff <= 5,
    TRUE(),
    FALSE()
)
