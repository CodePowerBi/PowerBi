Level2_con_TOTAL_ASSETS_CELIA :=
VAR _Is999 =
    SELECTEDVALUE ( Dicc_Epigrafes_Total[id_epigrafe] ) = 999
RETURN
IF (
    _Is999,
    CALCULATE (
        [Level2],
        -- IDs que componen el total
        KEEPFILTERS ( fusion_vw[id_epigrafe] IN { 6, 7, 10, 13 } ),
        -- QUITAR el 999 (clave para que no quede en blanco)
        REMOVEFILTERS ( Dicc_Epigrafes_Total[id_epigrafe] ),
        -- y si quieres que el total ignore el desglose por ep√≠grafe_front_total:
        REMOVEFILTERS ( Dicc_Epigrafes_Total[epigrafe_front_total] )
    ),
    CALCULATE (
        [Level2],
        KEEPFILTERS ( fusion_vw[id_epigrafe] IN { 6, 7, 10, 13 } )
    )
)
