Level1_Eje_Meses :=
VAR Cat    = SELECTEDVALUE ( Eje_Meses[Categoria] )
VAR Slicer = [MesMax_Slicer]
VAR TargetYM =
    SWITCH (
        Cat,
        "Actual", Slicer,
        "3m",     Slicer - 3,
        "6m",     Slicer - 6,
        "9m",     Slicer - 9,
        "12m",    Slicer - 12,
        BLANK()
    )
RETURN
IF (
    ISBLANK ( TargetYM ),
    BLANK(),
    CALCULATE (
        [Level1_con_TOTAL],
        'Dates'[MesAnioOrden] = TargetYM
    )
)
