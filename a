id_unit_count_or_one = 
VAR IdBuscado = 'tablones_des dicc_id_bsi'[id_id_bsi]
VAR CountDistinctValues =
    COUNTROWS(
        DISTINCT(
            SELECTCOLUMNS(
                FILTER(
                    'tablones_des usa_liquidez_vw',
                    'tablones_des usa_liquidez_vw'[id_id_bsi] = IdBuscado
                ),
                "id_unit", 'tablones_des usa_liquidez_vw'[id_unit]
            )
        )
    )
RETURN
IF(CountDistinctValues = 1, 1, CountDistinctValues)
