-- Medida para 1 mes anterior al seleccionado
Valor_Mes_Anterior_1 = 
VAR MesSeleccionado = SELECTEDVALUE(Dates[MesAñoUltMesTrimestre])
VAR ListaMeses = FILTER(ALL(Dates[MesAñoUltMesTrimestre]), NOT(ISBLANK(Dates[MesAñoUltMesTrimestre])))
VAR PosSeleccionado = RANKX(ListaMeses, Dates[MesAñoUltMesTrimestre], , DESC, DENSE)
VAR MesAnterior1 = 
    CALCULATE(
        MAX(Dates[MesAñoUltMesTrimestre]),
        FILTER(
            ListaMeses,
            RANKX(ListaMeses, Dates[MesAñoUltMesTrimestre], , DESC, DENSE) = PosSeleccionado + 1
        )
    )
RETURN
CALCULATE(
    SUM(km1[cantidad]),
    FILTER(
        ALL(Dates),
        Dates[MesAñoUltMesTrimestre] = MesAnterior1
    )
)
