Columna_FechaTrimestreAnterior = 
VAR FechaActualPais = 
    CALCULATE(
        MAX('tablones_des_vista_salida_daily_lcr_ddbb'[date]),
        ALLEXCEPT('tablones_des_vista_salida_daily_lcr_ddbb', 'tablones_des_vista_salida_daily_lcr_ddbb'[pais])
    )
VAR MesActual = MONTH(FechaActualPais)
VAR AnioActual = YEAR(FechaActualPais)
VAR TrimestreActual = INT((MesActual - 1) / 3) + 1

VAR PrimerMesTrimestreActual =
    SWITCH(
        TrimestreActual,
        1, 1,
        2, 4,
        3, 7,
        4, 10
    )

VAR MesAnterior = IF(PrimerMesTrimestreActual = 1, 12, PrimerMesTrimestreActual - 1)
VAR AnioAnterior = IF(PrimerMesTrimestreActual = 1, AnioActual - 1, AnioActual)

RETURN
    FORMAT(DATE(AnioAnterior, MesAnterior, 1), "MMM YY", "en-US")
