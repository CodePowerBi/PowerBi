InicioMes_Sel :=
VAR F = [FechaMax_Slicer PRUEBA2]
RETURN IF( NOT ISBLANK(F), DATE( YEAR(F), MONTH(F), 1 ) )

FinMes_Sel :=
VAR F = [FechaMax_Slicer PRUEBA2]
RETURN IF( NOT ISBLANK(F), EOMONTH( F, 0 ) )

-- Usa esta en Valores de la matriz
Valor_Metrica_MesSel :=
CALCULATE(
    SUM( 'salida_buffer_hqla_vw'[valor_metrica] ),
    DATESBETWEEN( Dates[Date], [InicioMes_Sel], [FinMes_Sel] )
)
