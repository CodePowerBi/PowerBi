TablaDespivotada =
UNION (
    SELECTCOLUMNS (
        'tablones_des_mrf_comprobaciones_eba',
        "effective_date", 'tablones_des_mrf_comprobaciones_eba'[effective_date],
        "pais", 'tablones_des_mrf_comprobaciones_eba'[pais],
        "metrica", "activos",
        "valor", 'tablones_des_mrf_comprobaciones_eba'[activos]
    ),
    SELECTCOLUMNS (
        'tablones_des_mrf_comprobaciones_eba',
        "effective_date", 'tablones_des_mrf_comprobaciones_eba'[effective_date],
        "pais", 'tablones_des_mrf_comprobaciones_eba'[pais],
        "metrica", "outflows",
        "valor", 'tablones_des_mrf_comprobaciones_eba'[outflows]
    ),
    SELECTCOLUMNS (
        'tablones_des_mrf_comprobaciones_eba',
        "effective_date", 'tablones_des_mrf_comprobaciones_eba'[effective_date],
        "pais", 'tablones_des_mrf_comprobaciones_eba'[pais],
        "metrica", "inflows",
        "valor", 'tablones_des_mrf_comprobaciones_eba'[inflows]
    ),
    SELECTCOLUMNS (
        'tablones_des_mrf_comprobaciones_eba',
        "effective_date", 'tablones_des_mrf_comprobaciones_eba'[effective_date],
        "pais", 'tablones_des_mrf_comprobaciones_eba'[pais],
        "metrica", "superavit",
        "valor", 'tablones_des_mrf_comprobaciones_eba'[superavit]
    ),
    SELECTCOLUMNS (
        'tablones_des_mrf_comprobaciones_eba',
        "effective_date", 'tablones_des_mrf_comprobaciones_eba'[effective_date],
        "pais", 'tablones_des_mrf_comprobaciones_eba'[pais],
        "metrica", "ratio",
        "valor", 'tablones_des_mrf_comprobaciones_eba'[ratio]
    )
)
