Valor YTD :=
VAR LastDate = MAX ( 'DimFecha'[Fecha] )
RETURN
CALCULATE(
    SUM ( 'main_sheet'[Valor] ),
    FILTER(
        ALL ( 'DimFecha' ),                         -- ignora el filtro de mes del slicer
        'DimFecha'[Fecha] <= LastDate
            && YEAR ( 'DimFecha'[Fecha] ) = YEAR ( LastDate )   -- mismo aÃ±o
    )
)
