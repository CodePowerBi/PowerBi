TotalAmountRecibidoPorFecha = 
CALCULATE(
    SUM('mrf_plantilla_67'[amount_recibido]),
    FILTER(
        'mrf_plantilla_67',
        'mrf_plantilla_67'[Counterparty ID] IN {232, 231, 49, 16, 92, 30, 175, 58, 115, 218}
        && 'mrf_plantilla_67'[Effective Date] = EARLIER('mrf_plantilla_67'[Effective Date])
    )
)
