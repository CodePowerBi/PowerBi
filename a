Public Ratio :=
VAR _metrics =
    { "COC [12M]", "COC 12M", "CoR 12M", "Cost of Credit (12M)" }
VAR v =
    CALCULATE(
        [Probando Valor.],
        KEEPFILTERS ( 'Data_vw_rva_provisions_forum'[metric] IN _metrics )
    )
RETURN
IF( ISBLANK(v), BLANK(), v / 100 )   -- tu fuente viene en base 100 (2.12 â†’ 0.0212)
