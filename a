Epigrafe_Corregido :=
SWITCH (
    TRUE(),
    'dicc_epigrafes'[id] IN {3, 9},        "Customers",
    'dicc_epigrafes'[id] = 2,              "Credit Institutions",
    'dicc_epigrafes'[id] = 11,             "Market Debt Securities",
    'dicc_epigrafes'[id] = 4,              "Debt Instruments",
    'dicc_epigrafes'[id] = 14,             "Subordinated Liabilities",
    'dicc_epigrafes'[id] = 13,             "Other Financial Liabilities",
    BLANK()
)
