next_meeting_nueva :=
VAR MaxYM =
    [MesMax_Slicer]
VAR FechaMeeting =
    CALCULATE (
        SELECTEDVALUE ( slide_info_vw[next_meeting] ),
        KEEPFILTERS ( 'A_Dates'[MesAnioOrden] = MaxYM )
    )
RETURN
IF (
    ISBLANK ( FechaMeeting ),
    BLANK(),
    LOWER ( FORMAT ( FechaMeeting, "dd-mmm" ) )   -- ej. 24-jul
)
