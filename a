mrf_lcr_historico =
VAR MaxFechaLCR =
    CALCULATE(
        MAX ( 'lcr_historico'[effective_date] ),
        ALL ( 'lcr_historico' )
    )

VAR FechaDateSeleccionada =
    MAX ( 'TABLA Effective Date'[Date] )

VAR UltimoMesLCR =
    EOMONTH ( MaxFechaLCR, 0 )

VAR UltimoMesDate =
    EOMONTH ( FechaDateSeleccionada, 0 )

VAR FechaFinal =
    MIN ( UltimoMesLCR, UltimoMesDate )

RETURN
    EOMONTH ( FechaDateSeleccionada, 0 ) = FechaFinal
