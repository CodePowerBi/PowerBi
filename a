Delta_vs_LastDec =
VAR CurrentDate = 'bank_ratio_cire_capital_vw'[effective_date]
VAR CurrentCurrency = 'bank_ratio_cire_capital_vw'[currency]
VAR LastDecValue =
    CALCULATE (
        SUM ( 'bank_ratio_cire_capital_vw'[gross_poss_cc] ),
        FILTER (
            ALL ( 'bank_ratio_cire_capital_vw' ),
            'bank_ratio_cire_capital_vw'[currency] = CurrentCurrency
                && YEAR ( 'bank_ratio_cire_capital_vw'[effective_date] ) = YEAR ( CurrentDate ) - 1
                && MONTH ( 'bank_ratio_cire_capital_vw'[effective_date] ) = 12
        )
    )
RETURN
'bank_ratio_cire_capital_vw'[gross_poss_cc] - LastDecValue
