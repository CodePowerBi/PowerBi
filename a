Color_Metrica_Español :=
VAR m = SELECTEDVALUE('dicc_metrica_esp'[metrica], "")
VAR m2 = UPPER(TRIM(m))
RETURN
IF (
    m2 = UPPER("Activos líquidos de alta calidad")
        || m2 = UPPER("Salidas de efectivo")
        || m2 = UPPER("Entradas de efectivo")
        || m2 = UPPER("TOTAL VALOR AJUSTADO"),
    "#E6E6E6",
    BLANK()
)
