Amount_MismoDia :=
VAR Fecha =
    'pag7_nueva'[Effective_date]
VAR CP =
    'pag7_nueva'[CounterpartyName]
RETURN
LOOKUPVALUE (
    Pag7_Top10_mensual[amount_received],          -- valor que quieres traer
    Pag7_Top10_mensual[fechha], Fecha,            -- buscar por fecha
    Pag7_Top10_mensual[counterparty_name], CP     -- y por counterparty
)
