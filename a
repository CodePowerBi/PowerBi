Comentario_Unido =
VAR Highs =
    CONCATENATEX (
        VALUES ( comment_js_vw[comentario high] ),
        comment_js_vw[comentario high],
        UNICHAR(13) & UNICHAR(10)
    )
VAR Eves =
    CONCATENATEX (
        VALUES ( comment_js_vw[comentario eve] ),
        comment_js_vw[comentario eve],
        UNICHAR(13) & UNICHAR(10)
    )
RETURN
Highs & 
IF ( Eves <> "", UNICHAR(13) & UNICHAR(10) & Eves, "" )
