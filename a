MesAÃ±oUltimoMesTrim_Otro = 
VAR AnyoActual = YEAR(TODAY())
VAR AnyoFecha = YEAR(Dates[Date])
VAR Mes = MONTH(Dates[Date])
VAR MesTexto = UPPER(LEFT(FORMAT(Dates[Date], "mmm"), 1)) & LOWER(MID(FORMAT(Dates[Date], "mmm"), 2, 2))
VAR AnyoTexto = RIGHT(FORMAT(Dates[Date], "yy"), 2)
RETURN
IF(
    AnyoFecha >= AnyoActual - 3,
    IF(
        Mes = 12,
        "Dic",
        BLANK()
    ),
    MesTexto & "-" & AnyoTexto
)
