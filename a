Suma_Mes_Anterior = 
VAR MesSeleccionado = SELECTEDVALUE(Dates[MesAñoUltMesTrimestre])
VAR ListaMeses = 
    FILTER(
        ALL(Dates[MesAñoUltMesTrimestre]],
        NOT(ISBLANK(Dates[MesAñoUltMesTrimestre]))
    )
VAR PosSeleccionado = 
    RANKX(
        ListaMeses,
        Dates[MesAñoUltMesTrimestre],
        ,
        DESC,
        DENSE
    )
VAR MesAnterior = 
    CALCULATE(
        MAX(Dates[MesAñoUltMesTrimestre]),
        FILTER(
            ListaMeses,
            RANKX(ListaMeses, Dates[MesAñoUltMesTrimestre], , DESC, DENSE) = PosSeleccionado + 1
        )
    )
RETURN
IF(
    NOT(ISBLANK(MesSeleccionado)) && NOT(ISBLANK(MesAnterior)),
    CALCULATE(
        SUM(km1[cantidad]),
        FILTER(
            ALL(km1),
            RELATED(Dates[MesAñoUltMesTrimestre]) = MesAnterior
        )
    ),
    BLANK()
)
