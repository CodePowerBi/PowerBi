Suma_Mes_Anterior = 
VAR MesSeleccionado = SELECTEDVALUE(km1[MesAñoUltMesTrimestre])
VAR ListaMeses = 
    DISTINCT(SELECTCOLUMNS(km1, "MesAño", km1[MesAñoUltMesTrimestre]))
VAR PosSeleccionado = 
    RANKX(
        ListaMeses,
        [MesAño],
        ,
        DESC,
        DENSE
    )
VAR MesAnterior = 
    MAXX(
        FILTER(
            ListaMeses,
            RANKX(ListaMeses, [MesAño], , DESC, DENSE) = PosSeleccionado + 1
        ),
        [MesAño]
    )
RETURN
IF(
    NOT(ISBLANK(MesSeleccionado)) && NOT(ISBLANK(MesAnterior)),
    CALCULATE(
        SUM(km1[cantidad]),
        km1[MesAñoUltMesTrimestre] = MesAnterior
    ),
    BLANK()
)
