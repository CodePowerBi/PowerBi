Medida_param_con_TOTAL :=
VAR EpigrafeEje =
    SELECTEDVALUE ( 'Dicc_Epigrafes_Total'[epigrafe_front] )
VAR Base = [Medida_param]   -- tu medida parametrizada
RETURN
IF (
    EpigrafeEje = "TOTAL",
    CALCULATE (
        Base,
        ALL ( 'Dicc_Epigrafes_Total'[epigrafe_front] )
    ),
    Base
)
