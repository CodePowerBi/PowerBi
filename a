Count_Residencia_FechaObjetivoIR :=
VAR FechaCtx =
    EOMONTH ( MAX ( 'Dates'[Date] ), 0 )

VAR FechaObjetivoIR =
    CALCULATE (
        MAX ( 'informe_regulatorio'[effective_date] ),
        REMOVEFILTERS ( 'informe_regulatorio' ),
        FILTER (
            ALL ( 'informe_regulatorio'[effective_date] ),
            'informe_regulatorio'[effective_date] <= FechaCtx
                && MONTH ( 'informe_regulatorio'[effective_date] ) IN { 3, 6, 9, 12 }
        )
    )

RETURN
CALCULATE (
    COUNT ( 'informe_regulatorio'[residence_id] ),     -- O la columna que uses
    REMOVEFILTERS ( 'Dates' ),
    TREATAS ( { FechaObjetivoIR }, 'Dates'[Date] )     -- Bloquea el contexto en esa fecha
)
