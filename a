Pag4_Calculada =
VAR HQLA =
    CALCULATETABLE (
        SELECTCOLUMNS (
            'tablones_des_mrf_comprobaciones_eba',
            "effective_date", 'tablones_des_mrf_comprobaciones_eba'[effective_date],
            "pais", 'tablones_des_mrf_comprobaciones_eba'[pais],
            "moneda", 'tablones_des_mrf_comprobaciones_eba'[hoja_limpio_moneda],
            "metrica", "HQLA",
            "valor", 'tablones_des_mrf_comprobaciones_eba'[activos]
        )
    )
VAR CashFlows =
    CALCULATETABLE (
        SELECTCOLUMNS (
            'tablones_des_mrf_comprobaciones_eba',
            "effective_date", 'tablones_des_mrf_comprobaciones_eba'[effective_date],
            "pais", 'tablones_des_mrf_comprobaciones_eba'[pais],
            "moneda", 'tablones_des_mrf_comprobaciones_eba'[hoja_limpio_moneda],
            "metrica", "CASH FLOWS",
            "valor", 
                'tablones_des_mrf_comprobaciones_eba'[outflows]
                - 'tablones_des_mrf_comprobaciones_eba'[inflows]
        )
    )
VAR SUP_DEF =
    GENERATE (
        SUMMARIZE (
            'tablones_des_mrf_comprobaciones_eba',
            'tablones_des_mrf_comprobaciones_eba'[effective_date],
            'tablones_des_mrf_comprobaciones_eba'[pais],
            'tablones_des_mrf_comprobaciones_eba'[hoja_limpio_moneda]
        ),
        VAR hqla_val =
            CALCULATE ( SUM ( 'tablones_des_mrf_comprobaciones_eba'[activos] ) )
        VAR cashflows_val =
            CALCULATE ( SUM ( 'tablones_des_mrf_comprobaciones_eba'[outflows] ) - SUM ( 'tablones_des_mrf_comprobaciones_eba'[inflows] ) )
        RETURN
            ROW (
                "effective_date", 'tablones_des_mrf_comprobaciones_eba'[effective_date],
                "pais", 'tablones_des_mrf_comprobaciones_eba'[pais],
                "moneda", 'tablones_des_mrf_comprobaciones_eba'[hoja_limpio_moneda],
                "metrica", "(SUP/DEF)",
                "valor", DIVIDE ( hqla_val, cashflows_val )
            )
    )
VAR Ratio =
    CALCULATETABLE (
        SELECTCOLUMNS (
            'tablones_des_mrf_comprobaciones_eba',
            "effective_date", 'tablones_des_mrf_comprobaciones_eba'[effective_date],
            "pais", 'tablones_des_mrf_comprobaciones_eba'[pais],
            "moneda", 'tablones_des_mrf_comprobaciones_eba'[hoja_limpio_moneda],
            "metrica", "RATIO",
            "valor", 'tablones_des_mrf_comprobaciones_eba'[ratio]
        )
    )
RETURN
    UNION ( HQLA, CashFlows, SUP_DEF, Ratio )
