Tabla_Grafico_Con_Other =
VAR total =
    CALCULATE(
        MAX('tablones_des_mrf_composition_per_currency_lcr_divisas'[sup_def]),
        'tablones_des_mrf_composition_per_currency_lcr_divisas'[divisa] = "TOT"
    )
VAR suma_6divisas =
    CALCULATE(
        SUM('tablones_des_mrf_composition_per_currency_lcr_divisas'[sup_def]),
        'tablones_des_mrf_composition_per_currency_lcr_divisas'[divisa] IN {
            "BRL", "CLP", "EUR", "GBP", "MXN", "USD"
        }
    )
VAR fecha =
    MAX('tablones_des_mrf_composition_per_currency_lcr_divisas'[effective_date])
RETURN
UNION(
    SELECTCOLUMNS(
        FILTER(
            'tablones_des_mrf_composition_per_currency_lcr_divisas',
            'tablones_des_mrf_composition_per_currency_lcr_divisas'[divisa] IN {
                "BRL", "CLP", "EUR", "GBP", "MXN", "USD"
            }
        ),
        "divisa", 'tablones_des_mrf_composition_per_currency_lcr_divisas'[divisa],
        "sup_def", 'tablones_des_mrf_composition_per_currency_lcr_divisas'[sup_def],
        "effective_date", 'tablones_des_mrf_composition_per_currency_lcr_divisas'[effective_date]
    ),
    ROW(
        "divisa", "OTHER",
        "sup_def", total - suma_6divisas,
        "effective_date", fecha
    )
)
