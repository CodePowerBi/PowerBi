Tabla_Grafico_Con_Other =
UNION(
    // Parte 1: las divisas normales
    SELECTCOLUMNS(
        FILTER(
            'tablones_des_mrf_composition_per_currency_lcr_divisas',
            'tablones_des_mrf_composition_per_currency_lcr_divisas'[divisa] IN { "BRL", "CLP", "EUR", "GBP", "MXN", "USD" }
        ),
        "effective_date", 'tablones_des_mrf_composition_per_currency_lcr_divisas'[effective_date],
        "divisa", 'tablones_des_mrf_composition_per_currency_lcr_divisas'[divisa],
        "sup_def", 'tablones_des_mrf_composition_per_currency_lcr_divisas'[sup_def]
    ),
    // Parte 2: OTHER por fecha
    SELECTCOLUMNS(
        ADDCOLUMNS(
            SUMMARIZE(
                'tablones_des_mrf_composition_per_currency_lcr_divisas',
                'tablones_des_mrf_composition_per_currency_lcr_divisas'[effective_date]
            ),
            "other_sup_def",
            CALCULATE(
                MAXX(
                    FILTER(
                        'tablones_des_mrf_composition_per_currency_lcr_divisas',
                        'tablones_des_mrf_composition_per_currency_lcr_divisas'[divisa] = "TOT"
                    ),
                    'tablones_des_mrf_composition_per_currency_lcr_divisas'[sup_def]
                )
                -
                CALCULATE(
                    SUM(
                        'tablones_des_mrf_composition_per_currency_lcr_divisas'[sup_def]
                    ),
                    'tablones_des_mrf_composition_per_currency_lcr_divisas'[divisa] IN { "BRL", "CLP", "EUR", "GBP", "MXN", "USD" }
                )
            )
        ),
        "effective_date", [effective_date],
        "divisa", "OTHER",
        "sup_def", [other_sup_def]
    )
)
