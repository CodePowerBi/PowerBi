Categoria 2 =
VAR vRaw = 'Tabla medidas'[campo]
VAR vNorm1 =
    SUBSTITUTE ( SUBSTITUTE ( SUBSTITUTE ( SUBSTITUTE ( SUBSTITUTE (
        vRaw,
        UNICHAR(160), " "      -- NBSP
    ), UNICHAR(8203), ""       -- zero-width space
    ), UNICHAR(8239), ""       -- narrow no-break
    ), UNICHAR(65279), ""      -- BOM
    ), UNICHAR(9), " " )       -- tab
VAR vClean = TRIM ( CLEAN ( vNorm1 ) )
RETURN
IF ( ISBLANK ( vRaw ) || vClean = "", 'Tabla medidas'[Categoria 1], vClean )
