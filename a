TOTAL COLUMNA Celeste prueba =
VAR ResultadoActual =
    DIVIDE (
        [Conjunto_1] + [Conjunto_2] + [Conjunto_3] + [Conjunto_4] + [Conjunto_5],
        1000
    )
VAR FechaActual = MAX ( 'TuTabla'[Fecha] )
VAR TrimestreAnterior =
    EOMONTH ( FechaActual, -3 )   -- Ãºltimo mes del trimestre anterior
VAR ResultadoAnterior =
    CALCULATE (
        DIVIDE (
            [Conjunto_1] + [Conjunto_2] + [Conjunto_3] + [Conjunto_4] + [Conjunto_5],
            1000
        ),
        FILTER (
            ALL ( 'TuTabla' ),
            'TuTabla'[Fecha] = TrimestreAnterior
        )
    )
RETURN
IF (
    ISBLANK ( ResultadoActual ),
    ResultadoAnterior,
    ResultadoActual
)
