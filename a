Valor con Duplicado =
VAR paisActual = SELECTEDVALUE('Duplicado_TotalGrupo'[Pais])
RETURN
SWITCH(
    TRUE(),
    paisActual = "Total Grupo (Duplicado)",
        CALCULATE(
            [Valor Formateado],
            KEEPFILTERS('dicc_pais'[Pais] = "Total Grupo")
        ),
    [Valor Formateado]
)
