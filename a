Over Outflows media porcentaje = 
DIVIDE(
    CALCULATE(
        SUM('tablones_des mrf_comprobaciones_eba_hbla'[three_notches]) +
        SUM('tablones_des mrf_comprobaciones_eba_hbla'[hlba])
    ),
    CALCULATE(
        SUM('tablones_des mrf_comprobaciones_eba'[outflows]),
        'tablones_des mrf_comprobaciones_eba'[MONEDA] IN { "USD", "MXN", "GBP", "EUR", "CLP", "BRL" },
        TREATAS(
            VALUES('tablones_des mrf_comprobaciones_eba_hbla'[effective_date]),
            'tablones_des mrf_comprobaciones_eba'[effective_date]
        ),
        TREATAS(
            VALUES('tablones_des mrf_comprobaciones_eba_hbla'[pais]),
            'tablones_des mrf_comprobaciones_eba'[pais]
        ),
        'tablones_des mrf_comprobaciones_eba'[sheet] = 1
    )
)
