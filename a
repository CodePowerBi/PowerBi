Título_position_menos_3meses =
VAR MesSeleccionado = SELECTEDVALUE(Dates[MesAñoUltMesTrimestre])
VAR FechaSeleccionada = 
    CALCULATE(
        MAX(Dates[Date]),
        Dates[MesAñoUltMesTrimestre] = MesSeleccionado
    )
VAR FechaMenos3M = EDATE(FechaSeleccionada, -3)
VAR Resultado =
    FORMAT(FechaMenos3M, "MMM") & "'" & FORMAT(FechaMenos3M, "yy")
RETURN
    UPPER(LEFT(Resultado,1)) & LOWER(MID(Resultado,2,3))


