Gross_poss_cc_YTD_Diff =
VAR LastDecValue =
    CALCULATE (
        MAX ( 'bank_ratio_cire_capital_vw'[gross_poss_cc] ),
        FILTER (
            ALL ( 'bank_ratio_cire_capital_vw' ),
            YEAR ( 'bank_ratio_cire_capital_vw'[date] ) = YEAR ( MAX ( 'bank_ratio_cire_capital_vw'[date] ) ) - 1
                && MONTH ( 'bank_ratio_cire_capital_vw'[date] ) = 12
        )
    )
VAR CurrentYTD =
    CALCULATE (
        SUM ( 'bank_ratio_cire_capital_vw'[gross_poss_cc] ),
        DATESYTD ( 'bank_ratio_cire_capital_vw'[date] )
    )
RETURN
CurrentYTD - LastDecValue
