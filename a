Valor_3M_Antes :=
VAR FechaActual = TablaResumen[report_date]
VAR Fecha_3M    = EDATE ( FechaActual, -3 )

VAR DivisaActual = TablaResumen[divisa]
VAR PaisActual   = TablaResumen[pais]   -- o id_pais, ajusta al tuyo
RETURN
CALCULATE (
    MAX ( TablaResumen[valor] ),        -- usa SUM si en esa tabla ya est√° agregado
    FILTER (
        ALL ( TablaResumen ),
        TablaResumen[report_date] = Fecha_3M
            && TablaResumen[divisa] = DivisaActual
            && TablaResumen[pais]   = PaisActual
    )
)
