RankingTop10_-3M :=
VAR ThisAmount = [Amount_-3M]
VAR ThisRank =
    RANKX (
        ALLSELECTED ( 'pag7 nueva'[CounterpartyName] ),
        [Amount_-3M],
        ThisAmount,
        DESC,
        DENSE
    )
RETURN
IF (
    ISBLANK ( ThisAmount ),
    BLANK (),
    IF ( ThisRank <= 10, ThisRank, BLANK() )
)
