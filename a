Mostrar_Hasta_Rango :=
VAR MaxFecha = [FechaMax_Slicer PRUEBA2]
VAR FechaEje = MAX ( 'Dates'[Date] )
VAR MinFecha = EDATE ( MaxFecha, -8 )   -- Max, Max-1, … Max-8  → 9 meses en total
RETURN
IF (
    NOT ISBLANK ( FechaEje ) &&
    NOT ISBLANK ( MaxFecha ) &&
    FechaEje <= MaxFecha &&
    FechaEje >= MinFecha,
    1,
    0
)
