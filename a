Ultimos6Meses = 
VAR MaxTexto = MAX('Dates'[Meses calculados])  -- AsegÃºrate de que apunta al campo correcto
VAR MaxFecha =
    IF(
        MaxTexto = "--" || ISBLANK(MaxTexto),
        BLANK(),
        DATE(
            2000 + VALUE(RIGHT(MaxTexto, 2)),
            SWITCH(LEFT(MaxTexto, 3),
                "ene", 1,
                "feb", 2,
                "mar", 3,
                "abr", 4,
                "may", 5,
                "jun", 6,
                "jul", 7,
                "ago", 8,
                "sep", 9,
                "oct", 10,
                "nov", 11,
                "dic", 12,
                BLANK()
            ),
            1
        )
    )

VAR FechaLimite = EOMONTH(MaxFecha, -5)
VAR FechaActual = 'Dates'[Date]

RETURN
IF (
    NOT ISBLANK(MaxFecha) &&
    FechaActual >= FechaLimite &&
    FechaActual <= MaxFecha,
    TRUE(),
    FALSE()
)
