ASSETS O LIABILITIES :=
IF (
    'Dicc_epigrafes_Total'[Limpieza new] = "TOTAL",
    "TOTAL",
    LOOKUPVALUE (
        dicc_epigrafes_filter_vw[Asset_Liabilities],   -- valor a devolver
        dicc_epigrafes_filter_vw[id_epigrafe],         -- columna clave en tabla origen
        'Dicc_epigrafes_Total'[id_epigrafe]            -- columna clave en esta tabla
    )
)
