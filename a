IsMaxMonthYear_Menos3Meses =
VAR MaxFecha = [Max]
VAR MaxFecha_Menos3 = EDATE(MaxFecha, -3)
VAR MesMax = MONTH(MaxFecha_Menos3)
VAR A単oMax = YEAR(MaxFecha_Menos3)
VAR MesFila = MONTH('Dates'[Date])
VAR A単oFila = YEAR('Dates'[Date])
RETURN
IF(
    MesFila = MesMax &&
    A単oFila = A単oMax,
    TRUE(),
    FALSE()
)
