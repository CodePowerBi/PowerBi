Mostrar_Hasta_Rango_9M := -- pruebita 1
VAR MaxYM =
    CALCULATE ( [MesMax_Slicer], ALL ( 'Dicc_epigrafes_total' ) )
VAR MinYM = MaxYM - 8
VAR YM = SELECTEDVALUE ( 'A_Dates'[MesAnioOrden] )
RETURN
IF (
    NOT ISINSCOPE ( 'A_Dates'[Monthly] ),   -- estamos en TOTAL/subtotal
    1,
    IF (
        NOT ISBLANK ( MaxYM )
            && NOT ISBLANK ( YM )
            && YM >= MinYM
            && YM <= MaxYM,
        1,
        0
    )
)
