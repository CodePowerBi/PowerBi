Mostrar_Hasta_Rango_9M :=
VAR MaxYM = [MesMax_Slicer]          -- p.ej. 202506 si eliges jun-25
VAR MinYM = MaxYM - 8                -- incluye Max, Max-1, â€¦ Max-8 -> 9 meses
VAR YM    = SELECTEDVALUE( 'Dates'[MesAnioOrden] )
RETURN
IF (
    NOT ISBLANK ( MaxYM ) &&
    NOT ISBLANK ( YM ) &&
    YM >= MinYM &&
    YM <= MaxYM,
    1,
    0
)
