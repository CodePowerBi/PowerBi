Ranking_-3M :=
VAR ThisAmount = [Amount_-3M]
RETURN
IF (
    ISBLANK ( ThisAmount ),
    BLANK (),
    RANKX (
        ALLSELECTED ( 'pag7 nueva'[CounterpartyName] ),   -- todas las contrapartes filtradas
        [Amount_-3M],                                     -- se ordenan por el amount de -3M
        ThisAmount,
        DESC,
        SKIP
    )
)
