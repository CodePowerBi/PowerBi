Dates = 
VAR FechaMin = MIN('salida_rwa'[fecha])
VAR FechaMax = TODAY()
RETURN
ADDCOLUMNS(
    CALENDAR(FechaMin, FechaMax),
    "Año", YEAR([Date]),
    "Mes", MONTH([Date]),
    "NombreMes", FORMAT([Date], "mmmm"),
    "Trimestre", "T" & FORMAT([Date], "Q"),
    "AñoMes", FORMAT([Date], "YYYY-MM")
)
