Mostrar_Hasta_Rango_3_slide19 :=
VAR MaxFecha = [FechaMax_Slicer PRUEBA2]
VAR FechaEje = MAX ( 'Dates'[Date] )
VAR MinFecha = EDATE ( MaxFecha, -9 )   -- Max, Max-1, â€¦ Max-8
VAR Epig     = SELECTEDVALUE ( 'Dicc_epigrafes_total'[Limpieza new] )
RETURN
IF (
    Epig = "TOTAL",
    1,                                      -- siempre mostrar TOTAL
    IF (
        NOT ISBLANK ( FechaEje ) &&
        NOT ISBLANK ( MaxFecha ) &&
        FechaEje <= MaxFecha &&
        FechaEje >= MinFecha,
        1,
        0
    )
)
