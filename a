Mostrar_Hasta_Rango_3_slide19 :=
VAR MaxYM =
    CALCULATE (
        [MesMax_Slicer],                       -- tu medida de mes máximo
        ALL ( 'Dicc_epigrafes_total' )         -- ignora epígrafe para calcular el max
    )
VAR MinYM = MaxYM - 8                          -- últimos 9 meses (Max, Max-1, … Max-8)
VAR YM    = SELECTEDVALUE ( 'Dates'[MesAnioOrden] )

-- epígrafe actual
VAR Epig =
    SELECTEDVALUE ( 'Dicc_epigrafes_total'[Limpieza new] )
    // o, si lo prefieres más seguro:
    // VAR EpigId = SELECTEDVALUE('Dicc_epigrafes_total[id_epigrafe])

RETURN
IF (
    Epig = "TOTAL",                            -- o EpigId = 999
    1,                                         -- siempre mostrar TOTAL
    IF (
        NOT ISBLANK ( MaxYM ) &&
        NOT ISBLANK ( YM ) &&
        YM >= MinYM &&
        YM <= MaxYM,
        1,
        0
    )
)
