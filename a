Level2_con_TOTAL :=
VAR Id = SELECTEDVALUE ( Dicc_Epigrafes_Total[id_epigrafe] )

-- IDs que quieres sumar cuando est√°s en la fila TOTAL (999)
VAR IdsTotal = { 3134, 35, 26, 22, 18, -16 }

VAR Res =
    IF (
        Id = 999,
        CALCULATE(
            [Level2_Base],
            REMOVEFILTERS ( Dicc_Epigrafes_Total[id_epigrafe] ),
            fusion_vw[id_epigrafe] IN IdsTotal
        ),
        [Level2_Base]
    )
RETURN
COALESCE ( Res, 0 )
