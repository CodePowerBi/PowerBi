12_arriba_3m_format_MesSel :=
VAR ValorMesSel =
    CALCULATE (
        SUM ( 'salida_buffer_vw'[valor] ),
        DATESBETWEEN (
            Dates[Date],
            [InicioMes_Sel PRUEBA 2 *],
            [FinMes_Sel PRUEBA 2]
        )
    )

VAR EsRatio =
    OR (
        SELECTEDVALUE ( 'km1'[Categoria_2_Nombre] ) = "Ratio de cobertura de liquidez (%)*",
        SELECTEDVALUE ( 'km1'[Categoria_2_Nombre] ) = "Ratio NSFR (%)"
    )

RETURN
IF (
    EsRatio,
    FORMAT ( ValorMesSel, "#%" ),
    ValorMesSel
)
