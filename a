LCR_12M_Chart :=
VAR MesAncla    = SELECTEDVALUE ( Dates[MesAnioOrden] )          -- Mes elegido en el slicer
VAR MesActual   = SELECTEDVALUE ( EjeMes_12M[MesAnioOrden] )     -- Mes de cada columna del gráfico
VAR DentroVentana =
    NOT ISBLANK ( MesAncla ) &&
    NOT ISBLANK ( MesActual ) &&
    MesActual <= MesAncla &&
    MesActual > MesAncla - 12        -- últimos 12 meses (MesAncla incluido)
RETURN
IF (
    NOT DentroVentana,
    BLANK(),                         -- Fuera del rango 12M, no mostramos nada
    CALCULATE (
        [LCR_Base],                  -- tu medida de LCR normal
        REMOVEFILTERS ( Dates ),     -- quitamos el filtro de fechas actual
        TREATAS ( { MesActual }, Dates[MesAnioOrden] )   -- fijamos el mes que corresponde a la columna del gráfico
    )
)
