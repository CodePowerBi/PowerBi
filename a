Valor_Mes_T_Menos1 = 
VAR FechaMax = 
    CALCULATE(
        MAX ( 'tablones_des_salida_buffer'[fecha] ),
        FILTER(
            ALL ( 'tablones_des_salida_buffer' ),
            'tablones_des_salida_buffer'[valor] <> 0
        )
    )
VAR PrimerDiaT   = DATE( YEAR(FechaMax), MONTH(FechaMax), 1 )
VAR PrimerDiaT1  = EDATE( PrimerDiaT, -1 )            // un mes atrÃ¡s
VAR FinMesT1     = EOMONTH( PrimerDiaT1, 0 )

RETURN
CALCULATE(
    SUM( 'tablones_des_salida_buffer'[valor] ),
    'tablones_des_salida_buffer'[fecha] >= PrimerDiaT1,
    'tablones_des_salida_buffer'[fecha] <= FinMesT1
)
