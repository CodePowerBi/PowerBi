WAIM_vlookup :=
VAR cpty = 'Pag7_Top10_mensual'[counterparty_name]
VAR mes  = 'Pag7_Top10_mensual'[fecha]  -- inicio de mes
RETURN
CALCULATE(
    MAX( 'tablones_des mrf_plantilla_67'[weight_average_initial_maturity] ),
    'tablones_des mrf_plantilla_67'[counterparty_name] = cpty,
    'tablones_des mrf_plantilla_67'[effective_date] >= mes,
    'tablones_des mrf_plantilla_67'[effective_date] <= EOMONTH(mes, 0)
)
