%Funding_fila :=
VAR amount = 'Pag7_Top10_mensual'[amount_received]          -- o tu columna [Amount - Millones]
VAR pasivo = 'Pag7_Top10_mensual'[CantidadTotalPasivo]
VAR total  = 'Pag7_Top10_mensual'[Amount_Month_Sum]         -- pon aquí tu “total month” real
VAR den    = COALESCE(pasivo,0) + COALESCE(total,0)
RETURN IF( OR(ISBLANK(amount), den = 0), BLANK(), amount / den )
