SumaUltimoMesTrimestreCompleto_EUR_mod =
VAR UltimaFechaCompleta =
    CALCULATE(
        MAX('tablones_des_salida_intragrupo_1'[process_date]),
        FILTER(
            'tablones_des_salida_intragrupo_1',
            MONTH('tablones_des_salida_intragrupo_1'[process_date]) IN {3, 6, 9, 12}
        )
    )
VAR UltimaMes = MONTH(UltimaFechaCompleta)
VAR UltimoAño = YEAR(UltimaFechaCompleta)
VAR SumaValor =
    CALCULATE(
        SUM('tablones_des_salida_intragrupo_1'[reop_prec_mens]),
        FILTER(
            'tablones_des_salida_intragrupo_1',
            MONTH('tablones_des_salida_intragrupo_1'[process_date]) = UltimaMes &&
            YEAR('tablones_des_salida_intragrupo_1'[process_date]) = UltimoAño
        )
    )
VAR ResultadoSuma =
    IF(
        ISBLANK(SumaValor) || SumaValor = 0,
        "-",
        FORMAT(SumaValor, "#,0")   // Convertimos la suma a texto con formato
    )
VAR NombreCPty = [aa_nombre_cpty]
VAR ResultadoCPty =
    IF(
        ISBLANK(NombreCPty) || NombreCPty = "",
        "-",
        NombreCPty
    )
RETURN
    /* Ejemplo 1: Si quieres que se evalúe primero si NombreCPty está vacío y, en ese caso, devuelva "-"  
       o, de lo contrario, se muestre el resultado de la suma (que ya tiene incorporada la sustitución de valores) */
    IF(ResultadoCPty = "-", "-", ResultadoSuma)
